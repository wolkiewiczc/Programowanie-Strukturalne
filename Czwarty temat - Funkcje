
#include<stdio.h>
#include<math.h>

NAPISANE FUNKCJE WKLEJAMY POWYZEJ MAIN, KTORY JE WYKONUJE

int wb(int n)
{
  if(n>=0)
  {
    return n;
  }
  else
  {
    return -n;
  }
}



int main()
{
    int n;
    scanf("%d",&n);
     printf("%d",wb(n));
}

